@{
    var clientBuildVersion = 20;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1' http-equiv='X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <meta name="description" content="A tool to help you generate the scripts needed to install Umbraco and the associated packages.">
    <meta property="og:image" content="https://psw.codeshare.co.uk/logo.png" />
    <meta property="og:image:type" content="image/png">
    <meta property="og:title" content="Package Script Writer" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content="A tool to help you generate the scripts needed to install Umbraco and the associated packages." />
    <meta property="og:url" content="https://psw.codeshare.co.uk" />
    <meta name="twitter:card" content="summary">
    <meta name="twitter:title" content="Package Script Writer">
    <meta name="twitter:description" content="A tool to help you generate the scripts needed to install Umbraco and the associated packages.">
    <meta name="twitter:image" content="https://psw.codeshare.co.uk/logo.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
    <title>Package Script Writer</title>
    <link rel="canonical" href="https://psw.codeshare.co.uk">
    <link rel="stylesheet preload" as="style" href="@Url.Content("~/portfolio-assets/bootstrap/dist/css/bootstrap.min.css")">
    <link rel="stylesheet preload" as="style" href="@Url.Content($"~/css/style.css?v={clientBuildVersion}")">
    <link rel="stylesheet preload" as="style" href="@Url.Content("~/portfolio-assets/ionicons.min.css")">
    <link rel="stylesheet preload" as="style" href="@Url.Content("~/css/sunburst.css")">
</head>
<body>
    @RenderBody()

    <script src="@Url.Content($"~/js/site.js?v={clientBuildVersion}")"></script>
    <script src="@Url.Content("~/portfolio-assets/bootstrap/dist/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/js/run_prettify.js?autoload=true&skin=sunburst")" defer></script>

    @await RenderSectionAsync("Scripts", required: false)

    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-3DVZWNLT7F"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-3DVZWNLT7F');
    </script>
</body>
</html>
